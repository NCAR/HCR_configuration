#! /bin/csh -f

# HCR QC processing
#
# Run RadxDwellCombine
# Reads 100 hz data in CfRadial files, combines into 10 hz dwells
# Writes 10hz CfRadial data

source ~/git/HCR_configuration/projDir/system/params/project_info.otrec

cd $PROJ_DIR/qc/params

RadxDwellCombine -params RadxDwellCombine.qc.10hz -debug -start "2019 08 07 00 00 00" -end "2019 10 07 00 00 00" >& /tmp/RadxDwellCombine.qc.10hz.otrec.log &

tail -f /tmp/RadxDwellCombine.qc.10hz.otrec.log

