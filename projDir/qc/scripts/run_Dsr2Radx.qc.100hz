#! /bin/csh -f

# HCR QC processing
#
# Run Dsr2Radx
# Reads FMQ from Iq2Dsr.qc.100hz
# Writes 100hz CfRadial data

source ~/git/HCR_configuration/projDir/qc/scripts/set_env_qc

cd $PROJ_DIR/qc/params

Dsr2Radx -params Dsr2Radx.qc.100hz -verbose >& /tmp/Dsr2Radx.qc.100hz.log &

tail -f /tmp/Dsr2Radx.qc.100hz.log



