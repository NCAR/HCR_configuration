#! /bin/csh

rehash

setenv PROJ_DIR /opt/local/git/HCR_configuration/projDir
setenv DATA_DIR /scr/rain1/rsfdata/projects
cd $PROJ_DIR/display/params

if ($#argv < 6) then
  /opt/local/lrose/bin/HawkEye -params HawkEye.noreaster.10hz >& /dev/null &
else
  /opt/local/lrose/bin/HawkEye -params HawkEye.noreaster.10hz -start_time "$1 $2 $3 $4 $5 $6" >& /dev/null &
endif

