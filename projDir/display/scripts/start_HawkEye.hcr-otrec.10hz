#! /bin/csh

rehash

setenv PROJ_DIR /opt/local/git/HCR_configuration/projDir
cd $PROJ_DIR/display/params

if ($#argv < 6) then
  /opt/local/lrose/bin/HawkEye -params HawkEye.hcr-otrec.10hz -start_time "2019 08 03 19 30 00" >& /dev/null &
else
  /opt/local/lrose/bin/HawkEye -params HawkEye.hcr-otrec.10hz -start_time "$1 $2 $3 $4 $5 $6" >& /dev/null &
endif

