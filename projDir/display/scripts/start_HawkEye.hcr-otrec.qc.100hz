#! /bin/csh

rehash

#setenv PROJ_DIR /opt/local/git/HCR_configuration/projDir

source ~/git/HCR_configuration/projDir/system/params/project_info.otrec

cd $PROJ_DIR/display/params

#set HAWKEYE = /opt/local/lrose/bin/HawkEye
set HAWKEYE = HawkEye

if ($#argv < 6) then
  $HAWKEYE -params HawkEye.hcr-otrec.qc.100hz -start_time "2019 08 07 15 30 00" >& /dev/null &
else
  $HAWKEYE -params HawkEye.hcr-otrec.qc.100hz -start_time "$1 $2 $3 $4 $5 $6" >& /dev/null &
endif

