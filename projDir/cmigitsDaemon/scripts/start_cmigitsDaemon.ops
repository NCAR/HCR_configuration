#! /bin/csh -f
#set ttyPort = "/dev/ttyS2"
set ttyPort = "/dev/ttyUSB0"
cd $HOME/git/HCR_instrument/src/cmigitsDaemon/

running "cmigitsDaemon $ttyPort"
if ($status == 0) then
    exit 0
endif

snuff cmigitsDaemon

limit coredumpsize unlimited
cmigitsDaemon $ttyPort |& \
    LogFilter -d $ERRORS_LOG_DIR -p cmigitsDaemon -i ops &

